{
  "name": "moon-phase-illuminated",
  "version": "2.2.0",
  "description": "Beautiful React moon phase component using real NASA imagery. Zero config, just pass a date.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:images": "node scripts/build-images.mjs",
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "moon",
    "moon-phase",
    "lunar",
    "astronomy",
    "nasa",
    "component"
  ],
  "author": "rishavgautam",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.5",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rishavgautam/react-moon-phase.git"
  }
}
